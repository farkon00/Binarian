{
    "name": "binarian",
    "displayName": "Binarian",
    "description": "Language support for binarian",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "binarian",
            "aliases": ["Binarian", "binarian"],
            "extensions": [".bino"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "binarian",
            "scopeName": "text.binarian",
            "path": "./syntaxes/binarian.tmLanguage.json"
        }]
    }
}